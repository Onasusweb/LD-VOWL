{
  "name": "linked-data-vowl",
  "version": "0.0.1",
  "description": "Extract ontologies from arbitrary SPARQL endpoints and visualize them using VOWL.",
  "main": "app/app.js",
  "dependencies": {
    "angular": "^1.4.8",
    "angular-animate": "^1.4.8",
    "angular-cookies": "^1.4.8",
    "angular-route": "^1.4.8",
    "angular-ui-bootstrap": "^0.14.3",
    "bootstrap": "^3.3.6",
    "bootstrap-webpack": "0.0.5",
    "d3": "^3.5.10",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "ng-cache-loader": "0.0.15",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.9",
    "babel-core": "^6.3.13",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.6.0",
    "eslint-config-angular": "^0.4.0",
    "eslint-plugin-angular": "^0.15.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai-plugins": "^0.6.1",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "0.0.10",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.19",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "scripts": {
    "start": "webpack-dev-server --content-base app --inline --hot & npm run proxy",
    "host": "webpack-dev-server --host 0.0.0.0 --content-base app --inline --hot",
    "proxy": "node ../anti-cors-proxy/index.js",
    "test": "karma start",
    "lint": "eslint . --ext .js || true",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js"
  },
  "keywords": [
    "VOWL"
  ],
  "author": "Marc Weise <weisemc@studi.informatik.uni-stuttgart.de>",
  "private": true,
  "license": "MIT",
  "_aerobatic": {
    "router": [
      {
        "module": "basic-auth",
        "path": "/",
        "options": {
          "username": "$BASIC_AUTH_USERNAME",
          "password": "$BASIC_AUTH_PASSWORD"
        }
      },
      {
        "module": "webpage",
        "options": {
          "pushState": true
        }
      },
      {
        "module": "client-settings",
        "options": {
          "showEndpointURL": "$SHOW_ENDPOINT_URL"
        }
      }
    ]
  }
}
